# Ejemplo: programa que imprime 3 n√∫meros ordenados

int a = 3;
int b = 4;
int c = 9;

if (a < b) {
    if (a < c) {
        print(a);
        if (b < c) { print (b); print(c); }
        else { print(c); print(b); }
    }
    else {
        print(c);
        print(a);
        print(b);
    }
}
else{
    if (b < c) {
        print(b);
        if (a < c) { print(a); print(c); }
        else { print(c); print(a); }
    }
    else{
        print(c);
        print(b);
        print(a);
    }
}
